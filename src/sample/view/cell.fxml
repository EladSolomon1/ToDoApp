<AnchorPane fx:id="rootAnchorPane" prefHeight="90.0" prefWidth="339.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <ImageView fx:id="iconImageView" fitHeight="44.0" fitWidth="60.0" layoutX="4.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icon_tasks.png" />
         </image>
      </ImageView>
      <Label fx:id="taskLabel" layoutX="48.0" layoutY="2.0" prefHeight="21.0" prefWidth="277.0" text="Task" textFill="WHITE">
