            <Image url="@../assets/icon_delete.png" />
         </image>
      </ImageView>
      <ImageView fx:id="listUpdateButton" fitHeight="32.0" fitWidth="28.0" layoutX="322.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
